name=mlibc
version=b587536d4ba6c7994e090b7ed6ccb9562463b39e
source_method=tarball
tarball_url="https://github.com/managarm/mlibc/archive/${version}.tar.gz"

regenerate() {
    for file in $(find . -type f); do sed -i 's/vinix/lyre/g' "$file"; done
    for file in $(find . -type f); do sed -i 's/VINIX/LYRE/g' "$file"; done
    for file in $(find . -type f); do sed -i 's/Vinix/Lyre/g' "$file"; done
    mv sysdeps/vinix sysdeps/lyre
}
